        "action": "reject"
      },
      {
        "query_type": [
          "A",
          "AAAA"
        ],
        "server": "dns_fakeip",
        "rewrite_ttl": 1
      },
      {
        "clash_mode": "Global",
        "server": "dns_proxy"
      },
      {
        "clash_mode": "Direct",
        "server": "dns_direct"
      },
      {
        "rule_set": [
          "geosite-cn"
        ],
        "server": "dns_direct"
      }
    ],
    "strategy": "ipv4_only",
    "independent_cache": true
